{
  "manifest_version": 3,
  "name": "Focus Hours",
  "description": "Focus Hours is a powerful extension that allows you to redirect blocked websites to URLs of your choice.",
  "version": "1.0",
  "permissions": ["webNavigation", "tabs", "storage"],
  "background": {
    "service_worker" : "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png",
    "48": "icon.png",
    "16": "icon.png"
  }
}